{
  "build": {
    "beforeDevCommand": "yarn workspace frontend dev",
    "beforeBuildCommand": "yarn workspace frontend build",
    "devPath": "http://localhost:1420",
    "distDir": "../frontend/dist"
  },
  "package": {
    "productName": "circles",
    "version": "0.1.7"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "save": true
      },
      "window": {
        "setFullscreen": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "be.pointsofview.circles",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": true,
        "resizable": true,
        "title": "circles",
        "width": 800,
        "height": 600
      }
    ],
    "updater": {
      "active": true,
      "endpoints": [
        "https://tree-projects.s3.eu-central-1.amazonaws.com/circles/update.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY2QTU2N0U1QzdDMEEyQjcKUldTM29zREg1V2VsOXRNMXIvbmZJbjRCSTZDQ1B5czZKekQrcDkwOThSYy9ENjVwaDAwcC9SZmIK"
    }
  }
}
